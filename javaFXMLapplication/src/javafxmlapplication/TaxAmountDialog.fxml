<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns:fx="http://javafx.com/fxml" fx:controller="javafxmlapplication.TaxAmountDialogController">
    <children>
        <Label text="TIN Number:" layoutX="14.0" layoutY="14.0"/>
        <TextField fx:id="tinNumberField" layoutX="120.0" layoutY="10.0"/>
        
        <Label text="Daily Income:" layoutX="14.0" layoutY="54.0"/>
        <TextField fx:id="dailyIncomeField" layoutX="120.0" layoutY="50.0"/>
        
        <Button text="Submit" layoutX="120.0" layoutY="90.0" onAction="#handleSubmit"/>
        <Button text="Cancel" layoutX="200.0" layoutY="90.0" onAction="#handleCancel"/>
    </children>
</AnchorPane>
